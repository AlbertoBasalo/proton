  "author": {
    "name": "Alberto Basalo",
    "email": "albertobasalo@hotmail.com",
    "url": "https://twitter.com/albertobasalo"
  },
  "homepage": "https://github.com/AtomicBuilders/proton#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AtomicBuilders/proton.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AtomicBuilders/proton/issues"
  },
  "scripts": {
    "start": "node ./dist/main.js",
    "prestart": "npm run build",
    "build": "tsc",
    "dev": "ts-node-dev ./src/main.ts",
    "test": "jest",
    "test:watch": "jest --watch --verbose",
    "test:coverage": "jest --coverage",
    "format": "prettier --write \"./**/*.{ts,json}\"",
    "lint": "eslint src --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "prerelease": "standard-version ",
    "release": "git push --follow-tags origin master",
    "updates": "ncu -u"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.20",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "npm-check-updates": "^10.2.5",
    "prettier": "^2.2.1",
    "standard-version": "^9.1.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "morgan-body": "^2.6.4",
    "typescript": "^4.1.3",
    "winston": "^3.3.3"
  }
}
